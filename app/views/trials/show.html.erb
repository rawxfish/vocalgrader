<%= @trial.name %>

<h3>Expected</h3>

<% if ExpectedWave.where(:trial_id => @trial.id).exists? %>
  <%= ExpectedWave.find(:trial_id => @trial.id) %>
<% else %>
  <%= form_for Trial.new, :html => { :mutlipart => true } do |f| %>
    <%= f.file_field :file %>
    <%= f.hidden_field :trial_id, @trial.id %>
    <%= f.submit "Submit" %>
  <% end %>
<% end %>


<h3>Trials</h3>
